tmux: C-b t show clock
%
tmux: C-b " split window into top and bottom
%
tmux: C-b % split window into left and right
%
tmux: C-b <arrow> switch to pane in that direction
%
tmux: C-b C-<arrow> grow pane in that direction
%
tmux: C-b M-<arrow> grow the pane in that direction by 5
%
tmux: C-b d detach from the session without killing it
%
tmux: C-b / Describe key binding
%
tmux: C-b ; Move to the previously active pane
%
tmux: C-b C-o Rotate through the panes
%
tmux: C-b space Select next layout
%
tmux: C-b $ Rename current session
%
tmux: C-b C-b  Send the prefix key
%
tmux: C-b C-o  Rotate through the panes
%
tmux: C-b C-z  Suspend the current client
%
tmux: C-b Space  Select next layout
%
tmux: C-b !  Break pane to a new window
%
tmux: C-b "  Split window vertically
%
tmux: C-b #  List all paste buffers
%
tmux: C-b $  Rename current session
%
tmux: C-b %  Split window horizontally
%
tmux: C-b &  Kill current window
%
tmux: C-b '  Prompt for window index to select
%
tmux: C-b (  Switch to previous client
%
tmux: C-b )  Switch to next client
%
tmux: C-b ,  Rename current window
%
tmux: C-b -  Delete the most recent paste buffer
%
tmux: C-b .  Move the current window
%
tmux: C-b /  Describe key binding
%
tmux: C-b [0-9]  Select window 0-9
%
tmux: C-b :  Prompt for a command
%
tmux: C-b ;  Move to the previously active pane
%
tmux: C-b =  Choose a paste buffer from a list
%
tmux: C-b ?  List key bindings
%
tmux: C-b D  Choose a client from a list
%
tmux: C-b E  Spread panes out evenly
%
tmux: C-b L  Switch to the last client
%
tmux: C-b M  Clear the marked pane
%
tmux: C-b [  Enter copy mode
%
tmux: C-b ]  Paste the most recent paste buffer
%
tmux: C-b c  Create a new window
%
tmux: C-b d  Detach the current client
%
tmux: C-b f  Search for a pane
%
tmux: C-b i  Display window information
%
tmux: C-b l  Select the previously current window
%
tmux: C-b m  Toggle the marked pane
%
tmux: C-b n  Select the next window
%
tmux: C-b o  Select the next pane
%
tmux: C-b p  Select the previous pane
%
tmux: C-b q  Display pane numbers
%
tmux: C-b r  Redraw the current client
%
tmux: C-b s  Choose a session from a list
%
tmux: C-b t  Show a clock
%
tmux: C-b w  Choose a window from a list
%
tmux: C-b x  Kill the active pane
%
tmux: C-b z  Zoom the active pane
%
tmux: C-b {  Swap the active pane with the pane above
%
tmux: C-b }  Swap the active pane with the pane below
%
tmux: C-b ~  Show messages
%
tmux: C-b DC  Reset so the visible part of the window follows the cursor
%
tmux: C-b PPage  Enter copy mode and scroll up
%
tmux: C-b M-1  Set the even-horizontal layout
%
tmux: C-b M-2  Set the even-vertical layout
%
tmux: C-b M-3  Set the main-horizontal layout
%
tmux: C-b M-4  Set the main-vertical layout
%
tmux: C-b M-5  Select the tiled layout
%
tmux: C-b M-n  Select the next window with an alert
%
tmux: C-b M-o  Rotate through the panes in reverse
%
tmux: C-b M-p  Select the previous window with an alert
%
